    {
      "name": "Fraud Detection",
      "item": [
        {
          "name": "Get Fraud Statistics",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/fraud/statistics/{{user_id}}",
              "host": ["{{base_url}}"],
              "path": ["fraud", "statistics", "{{user_id}}"]
            }
          }
        },
        {
          "name": "Get User Fraud Alerts",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/fraud/alerts/{{user_id}}",
              "host": ["{{base_url}}"],
              "path": ["fraud", "alerts", "{{user_id}}"]
            }
          }
        },
        {
          "name": "Get Unreviewed Alerts",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/fraud/alerts/unreviewed",
              "host": ["{{base_url}}"],
              "path": ["fraud", "alerts", "unreviewed"]
            }
          }
        },
        {
          "name": "Review Fraud Alert",
          "request": {
            "method": "PUT",
            "header": [],
            "url": {
              "raw": "{{base_url}}/fraud/alerts/1/review?reviewedBy=admin&confirmedFraud=true&comments=Verified",
              "host": ["{{base_url}}"],
              "path": ["fraud", "alerts", "1", "review"],
              "query": [
                {
                  "key": "reviewedBy",
                  "value": "admin"
                },
                {
                  "key": "confirmedFraud",
                  "value": "true"
                },
                {
                  "key": "comments",
                  "value": "Verified"
                }
              ]
            }
          }
        }
      ]
